{
    "search.exclude": {
        "**/*.g.dart": true,
        "**/*.freezed.dart": true,
        "**/.fvm": true,
        "lib/l10n/gen/*": true
    },
    "files.watcherExclude": {
        "**/.fvm": true
    },
    "dart.flutterSdkPath": ".fvm/flutter_sdk",
