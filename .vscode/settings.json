        ".fvm/flutter_sdk"
    ],
    "editor.formatOnSave": true,
    "files.exclude": {
        "**/*.freezed.dart": true,
        "**/*.g.dart": true,
        "**/generated_plugin_registrant.dart": true,
        "**/.flutter-plugins*": true,
        "**/.packages": true,
    }
}